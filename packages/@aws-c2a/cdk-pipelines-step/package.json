{
  "name": "@aws-c2a/cdk-pipelines-step",
  "stability": "experimental",
  "version": "0.3.3",
  "description": "A CDK Pipelines Step that runs aws-c2a diff given a rule set",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "jsii": {
    "outdir": "dist",
    "targets": {
      "java": {
        "package": "software.amazon.awscdk.c2a.pipelinestep",
        "maven": {
          "groupId": "software.amazon.awscdk",
          "artifactId": "c2a"
        }
      },
      "python": {
        "distName": "aws-cdk.c2a",
        "module": "aws_cdk.c2a"
      },
      "dotnet": {
        "namespace": "Amazon.CDK.AWS.C2A",
        "packageId": "Amazon.CDK.AWS.C2A"
      }
    }
  },
  "scripts": {
    "build": "yarn clean && yarn compile",
    "build:watch": "yarn clean && npx jsii --watch",
    "package": "jsii-pacmak",
    "compile": "npx jsii",
    "clean": "npx tsc --build --clean",
    "clean:all": "git clean -fdx",
    "lint": "eslint . --ext .ts",
    "test": "npx jest"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cdklabs/awscdk-change-analyzer.git",
    "directory": "packages/@aws-c2a/cdk-pipelines-step"
  },
  "author": {
    "name": "Amazon Web Services",
    "url": "https://aws.amazon.com",
    "organization": true
  },
  "keywords": [
    "aws",
    "cdk",
    "aws-cdk"
  ],
  "engines": {
    "node": ">= 10.13.0 <13 || >=13.7.0"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "@aws-cdk/aws-codebuild": "^1.115.0",
    "@aws-cdk/aws-codepipeline": "^1.115.0",
    "@aws-cdk/aws-codepipeline-actions": "^1.115.0",
    "@aws-cdk/aws-iam": "^1.115.0",
    "@aws-cdk/aws-lambda": "^1.115.0",
    "@aws-cdk/aws-s3": "^1.115.0",
    "@aws-cdk/aws-secretsmanager": "^1.115.0",
    "@aws-cdk/aws-sns": "^1.115.0",
    "@aws-cdk/cfnspec": "^1.115.0",
    "@aws-cdk/core": "^1.115.0",
    "@aws-cdk/pipelines": "^1.115.0",
    "constructs": "^3.3.69"
  },
  "devDependencies": {
    "@aws-cdk/assert": "^1.115.0",
    "@aws-cdk/cx-api": "1.115.0",
    "@babel/core": "^7.12.13",
    "@babel/preset-env": "^7.12.13",
    "@babel/preset-typescript": "^7.12.13",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.33",
    "@types/string-similarity": "^4.0.0",
    "@typescript-eslint/eslint-plugin": "^4.14.2",
    "@typescript-eslint/parser": "^4.14.2",
    "babel-jest": "^26.6.3",
    "eslint": "^7.19.0",
    "eslint-plugin-import": "^2.23.4",
    "jest": "^26.5.0",
    "jsii": "^1.33.0",
    "jsii-pacmak": "^1.33.0",
    "ts-jest": "^26.5.0",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.1.3"
  },
  "peerDependencies": {
    "@aws-cdk/aws-codebuild": "^1.115.0",
    "@aws-cdk/aws-codepipeline": "^1.115.0",
    "@aws-cdk/aws-codepipeline-actions": "^1.115.0",
    "@aws-cdk/aws-iam": "^1.115.0",
    "@aws-cdk/aws-lambda": "^1.115.0",
    "@aws-cdk/aws-s3": "^1.115.0",
    "@aws-cdk/aws-secretsmanager": "^1.115.0",
    "@aws-cdk/aws-sns": "^1.115.0",
    "@aws-cdk/core": "^1.115.0",
    "@aws-cdk/pipelines": "^1.115.0",
    "constructs": "^3.3.69"
  },
  "bundledDependencies": [
    "@aws-cdk/cfnspec"
  ],
  "publishConfig": {
    "access": "public"
  }
}
